<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.gsb.visprat">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="VisPratApplication"
        android:usesCleartextTraffic="true">
        <activity
            android:name="fr.gsb.visprat.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="fr.gsb.visprat.DeptsActivity"
            android:label="@string/title_activity_depts" >
        </activity>
        <activity
            android:name="fr.gsb.visprat.MedecinsActivity"
            android:label="@string/title_activity_medecins" >
        </activity>
        <activity
            android:name="fr.gsb.visprat.DetailMedecinActivity"
            android:label="@string/title_activity_medecins" >
        </activity>
        <activity
            android:name="fr.gsb.visprat.settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="fr.gsb.visprat.AddRapportVisiteActivity"
            android:label="@string/title_activity_add_rapports_visites" >
        </activity>

    </application>
</manifest>
